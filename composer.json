{
    "name": "qc/qc-redirects",
    "type": "typo3-cms-extension",
    "description": "Extends Core's Redirects Module with a Title column, showing creation and modification date and more sorting and filtering options. Also add an import redirects functionality.",
    "license": ["GPL-2.0-or-later"],
    "keywords": ["TYPO3 CMS"],
    "require": {
        "typo3/cms-core": "^10.4 || ^11.5",
        "typo3/cms-backend": "^10.4 || ^11.5",
        "typo3/cms-fluid": "^10.4 || ^11.5",
        "typo3/cms-redirects": "^10.4 || ^11.5"
    },
    "autoload": {
        "psr-4": {
            "Qc\\QcRedirects\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "qc_redirects"
        }
    }
}
